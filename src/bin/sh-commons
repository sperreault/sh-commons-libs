#!/usr/bin/env ksh
COMMONS_BASEDIR=@DESTDIR@
source ${COMMONS_BASEDIR}/lib/sh-commons.ksh

commons_load_module cmd ${0}

help() {
  echo "$(commons_cmd_script_name ${COMMONS_CMD_BASE_CMD}) as the following sub-commands:"
  commons_cmd_sub_commands
}

run() {
  help
}

commons_cmd_run ${@}

commons_cleanup
